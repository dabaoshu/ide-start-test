{
  "name": "opensumi-module-samples",
  "version": "1.0.0",
  "description": "Sample code illustrating the OpenSumi modules.",
  "main": "index.js",
  "repository": "git@github.com:opensumi/opensumi-module-samples.git",
  "license": "MIT",
  "scripts": {
    "postinstall": "npm run prepare",
    "start:client": "NODE_ENV=development webpack-dev-server --config ./opensumi-sample/configs/webpack.browser.config.js --progress --color --watch",
    "start:webview": "webpack-dev-server --config ./opensumi-sample/configs/webpack.webview.config.js",
    "start:server": "EXT_MODE=js KTLOG_SHOW_DEBUG=1 node --inspect=9991 -r ts-node/register -r tsconfig-paths/register ./opensumi-sample/src/node/index.ts",
    "compile:worker-host": "NODE_ENV=production webpack --config ./opensumi-sample/configs/webpack.worker-host.config.js --progress --color",
    "compile:ext-host": "NODE_ENV=production webpack --config ./opensumi-sample/configs/webpack.ext-host.config.js --progress --color",
    "prepare": "npm run compile:worker-host && npm run compile:ext-host && DEBUG=install-ext npm run download:extensions",
    "startup": "run-p start:client start:server start:webview"
  },
  "dependencies": {
